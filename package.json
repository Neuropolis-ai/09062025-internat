    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.2",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.7.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
